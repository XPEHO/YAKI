    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.16.16",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vitest/coverage-v8": "^0.32.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
